{
  "name": "@timbenniks/contentstack-endpoints",
  "version": "1.0.16",
  "description": "A utility package to get Contentstack API endpoints based on cloud and region",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "format": "oxc-fmt",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "contentstack",
    "api",
    "endpoints",
    "cms",
    "headless",
    "headless-cms"
  ],
  "author": "Tim Benniks",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timbenniks/contentstack-endpoints.git"
  },
  "bugs": {
    "url": "https://github.com/timbenniks/contentstack-endpoints/issues"
  },
  "homepage": "https://github.com/timbenniks/contentstack-endpoints#readme",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.0.10",
    "oxlint": "^1.11.2",
    "tsdown": "^0.14.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
