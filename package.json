{
  "name": "@timbenniks/contentstack-endpoints",
  "version": "1.0.15",
  "description": "A utility package to get Contentstack API endpoints based on cloud and region",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:js && npm run build:types",
    "build:js": "rolldown -c rolldown.config.js",
    "build:types": "tsdown --dts --out-dir dist/types src/index.ts",
    "build:vite": "npm run clean && vite build",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "format": "oxc-fmt",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "contentstack",
    "api",
    "endpoints",
    "cms",
    "headless",
    "headless-cms"
  ],
  "author": "Tim Benniks",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timbenniks/contentstack-endpoints.git"
  },
  "bugs": {
    "url": "https://github.com/timbenniks/contentstack-endpoints/issues"
  },
  "homepage": "https://github.com/timbenniks/contentstack-endpoints#readme",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.0.10",
    "oxlint": "^1.11.2",
    "rimraf": "^6.0.1",
    "rolldown": "^1.0.0-beta.32",
    "tsdown": "^0.14.1",
    "typescript": "^5.8.3",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^3.2.4"
  }
}
